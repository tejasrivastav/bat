    "gulp-nodemon"                                  : "~2.0.6",
    "gulp-rename"                                   : "~1.2.2",
    "gulp-rev"                                      : "~7.0.0",
    "gulp-sass"                                     : "~2.2.0",
    "gulp-streamify"                                : "~1.0.2",
